    "__split_buffer": "cpp",
    "__verbose_abort": "cpp",
    "ios": "cpp",
    "locale": "cpp",
    "__locale": "cpp",
    "__threading_support": "cpp",
    "__tree": "cpp",
    "queue": "cpp",
    "csetjmp": "cpp",
    "*.ipp": "cpp",
    "scoped_allocator": "cpp",
    "span": "cpp",
    "format": "cpp",
    "stack": "cpp",
    "rope": "cpp"
